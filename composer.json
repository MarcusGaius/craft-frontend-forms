{
	"name": "marcusgaius/craft-utilities",
	"description": "Various craftcms utilities.",
	"type": "yii2-extension",
	"version": "1.0.2",
	"require": {
		"craftcms/cms": "^4.4",
		"php": ">=8.1"
	},
	"require-dev": {
		"craftcms/ecs": "dev-main",
		"craftcms/phpstan": "dev-main"
	},
	"config": {
		"allow-plugins": {
			"craftcms/plugin-installer": true,
			"yiisoft/yii2-composer": true
		},
		"optimize-autoloader": true,
		"sort-packages": true
	},
	"license": "MIT",
	"autoload": {
		"psr-4": {
			"MarcusGaius\\Utilities\\": "src/"
		}
	},
	"authors": [
		{
			"name": "Marko GajiÄ‡",
			"email": "metalmorgoth@gmail.com",
			"homepage": "https://github.com/MarcusGaius"
		}
	],
	"extra": {
		"bootstrap": "MarcusGaius\\Utilities\\Utilities"
	}
}